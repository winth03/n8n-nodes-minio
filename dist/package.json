{
    "name": "@winth03/n8n-nodes-minio",
    "version": "1.0.0",
    "description": "n8n MinIO Node",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "https://github.com/winth03",
    "author": {
        "name": "Nutpapop Yasawut",
        "email": "nutpapop.win@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/winth03/n8n-nodes-minio.git"
    },
    "engines": {
        "node": ">=20.15"
    },
    "main": "index.js",
    "scripts": {
        "build": "npx rimraf dist && tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
        "prepare": "husky"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/MinIoApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/MinIO/MinIo.node.js"
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/node": "^24.2.1",
        "@typescript-eslint/parser": "~8.32.0",
        "eslint": "^8.57.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.3",
        "gulp": "^5.0.0",
        "husky": "^9.1.7",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "minio": "^8.0.5"
    }
}
